{
  "mcpServers": {
    "artl-mcp": {
      "command": "uvx",
      "args": ["artl-mcp"],
      "tools": ["*"]
    },
    "ols4": {
      "type": "http",
      "url": "http://www.ebi.ac.uk/ols4/api/mcp",
      "tools": ["*"]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
     "tools": ["*"]
    }
  }
}
